{
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", "tab"],
		"linebreak-style": ["error", "windows"],
		"quotes": ["error", "single"],
		"semi": ["error", "never"],
		"no-unused-vars": 0,
		"no-console": 0,
		"operator-linebreak": [2, "before", {"overrides": {"?": "after"}}]
	}
}
